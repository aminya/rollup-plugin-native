{
  "name": "rollup-plugin-native",
  "version": "0.0.6",
  "description": "Import native code with Rollup",
  "main": "dist/index.js",
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "npm run prepare",
    "test": "ava"
  },
  "keywords": [
    "rollup",
    "plugin",
    "native"
  ],
  "peerDependencies": {
    "rollup": "^1.27.1"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "esm": "^3.2.25",
    "rollup": "^1.27.1"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  }
}
