{
  "name": "rollup-plugin-native",
  "version": "0.0.0-semantic-release",
  "description": "Import native code with Rollup",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "npm run prepare",
    "test": "ava"
  },
  "keywords": [
    "rollup",
    "plugin"
  ],
  "peerDependencies": {
    "rollup": "^1.27.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "rollup": "^1.27.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  }
}
