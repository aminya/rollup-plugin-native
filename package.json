{
  "name": "rollup-plugin-native",
  "version": "0.0.9",
  "description": "Import native code with Rollup",
  "main": "dist/index.js",
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "keywords": [
    "rollup",
    "plugin",
    "native"
  ],
  "dependencies": {
    "rollup-pluginutils": "^2.8.2"
  },
  "peerDependencies": {
    "rollup": "^1.27.4"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "rollup": "^1.27.4"
  },
  "ava": {
    "extensions": [
      "js"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "build/*-test.js"
    ]
  }
}
