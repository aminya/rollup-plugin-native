{
  "name": "rollup-plugin-native",
  "version": "0.0.0-semantic-release",
  "description": "Import native code with Rollup",
  "main": "dist/index.js",
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/helpers/rollup.config.js && rollup -c tests/fixtures/rollup.config.js",
    "precover": "rollup -c tests/helpers/rollup.config.js && rollup -c tests/fixtures/rollup.config.js",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "ava"
  },
  "keywords": [
    "rollup-plugin",
    "native",
    "napi"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^3.0.8",
    "read-pkg-up": "^7.0.1"
  },
  "peerDependencies": {
    "rollup": "^1.31.1"
  },
  "devDependencies": {
    "ava": "^3.4.0",
    "c8": "^7.0.1",
    "execa": "^4.0.0",
    "rollup": "^1.31.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/rollup-plugin-native.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rollup-plugin-native/issues"
  },
  "homepage": "https://github.com/arlac77/rollup-plugin-native#readme"
}
