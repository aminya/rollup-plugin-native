{
  "name": "rollup-plugin-native",
  "version": "0.0.12",
  "description": "Import native code with Rollup",
  "main": "dist/index.js",
  "files": [
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "ava"
  },
  "keywords": [
    "rollup",
    "plugin",
    "native"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^3.0.0"
  },
  "peerDependencies": {
    "rollup": "^1.27.4"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "rollup": "^1.27.5",
    "c8": "^6.0.1"
  },
  "ava": {
    "extensions": [
      "js"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "build/*-test.js"
    ]
  }
}
